model OrderItem {
  id             String    @id @default(uuid())
  orderId        String
  medicineId     String
  quantity       Int
  priceAtPurchase Decimal  @db.Decimal(10,2)

  order          Order     @relation(fields: [orderId], references: [id])
  medicine       Medicine  @relation(fields: [medicineId], references: [id])
}